{
  "name": "react-nipplejs",
  "version": "0.0.4",
  "private": false,
  "description": "nipplejs wrapped in react",
  "homepage": "https://bstdevices.github.io/react-nipplejs",
  "repository": "github:bstdevices/react-nipplejs",
  "keywords": [
    "nipplejs",
    "react",
    "component",
    "components",
    "ecosystem-react",
    "react-component"
  ],
  "author": "Ola Malmgren",
  "license": "MIT",
  "sideEffects": false,
  "bugs": {
    "url": "https://github.com/bstdevices/react-nipplejs/issues"
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "build": "tsup src/index.tsx --format cjs --dts --external react",
    "clean": "rm -rf dist",
    "dev": "tsup src/index.tsx --format cjs --watch --dts --external react",
    "lint": "eslint \"src/**/*.ts*\""
  },
  "devDependencies": {
    "@types/react": "^17.0.13",
    "@types/react-dom": "^17.0.8",
    "eslint": "^8.4.1",
    "react": "^17.0.2",
    "tsup": "^5.10.1",
    "typescript": "^4.5.3"
  },
  "peerDependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "nipplejs": "^0.10.1"
  }
}
